<h1>Listing vals</h1>

<table>
  <tr>
    <th>P1</th>
    <th>P2</th>
    <th>P3</th>
    <th>P4</th>
    <th>P5</th>
    <th>P6</th>
    <th>P7</th>
    <th>P8</th>
    <th>P9</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @vals.each do |val| %>
  <tr>
    <td><%= val.p1 %></td>
    <td><%= val.p2 %></td>
    <td><%= val.p3 %></td>
    <td><%= val.p4 %></td>
    <td><%= val.p5 %></td>
    <td><%= val.p6 %></td>
    <td><%= val.p7 %></td>
    <td><%= val.p8 %></td>
    <td><%= val.p9 %></td>
    <td><%= link_to 'Show', val %></td>
    <td><%= link_to 'Edit', edit_val_path(val) %></td>
    <td><%= link_to 'Destroy', val, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

